{
  "name": "s2-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "d3": "^6.2.0",
    "d3-format": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "mapbox-gl": "^1.12.0",
    "marked": "^1.2.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.33.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "sander": "^0.6.0",
    "svelte": "^3.29.7",
    "topojson-client": "^3.1.0"
  },
  "dependencies": {
    "sirv-cli": "^1.0.8"
  },
  "scripts": {
    "autobuild": "rollup -c -w",
    "build": ". .env && npm run static && rollup -c",
    "dev": ". .env && run-p start:dev autobuild",
    "start": ". .env && sirv public --single",
    "start:dev": " sirv public --single --dev",
    "static:geo": "mkdir -p public/geo && cp -rf src/geo/* public/geo",
    "static:data": "mkdir -p public/data && cp -rf src/data/* public/data",
    "static": "run-p static:*"
  },
  "browserslist": "> 0.25%, not dead"
}
